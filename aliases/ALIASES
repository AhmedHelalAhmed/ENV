alias h='/snap/bin/heroku'

# for rvm
PATH=$PATH:.rvm/bin

#new terminal
PATH=/bin:/usr/bin:/usr/local/bin:${PATH}
export PATH
exec /usr/bin/zsh

#custom terminal symbol
PS1=' \w  <- '
